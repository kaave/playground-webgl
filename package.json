{
  "name": "playground-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "sideEffects": [
    "*.css"
  ],
  "browserslist": [
    "> 0.5% in jp",
    "not ios < 10.3",
    "not android < 5",
    "not safari < 10.1",
    "not chrome 49",
    "not dead"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.ts": [
        "tslint --fix",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "start": "run-p start:*",
    "start:gulp": "cross-env NODE_ENV=development gulp",
    "start:webpack": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --history-api-fallback --progress --color --config tools/webpack/development.js",
    "prebuild": "run-s clean:build",
    "build": "run-s build:gulp build:webpack",
    "build:gulp": "cross-env NODE_ENV=production gulp build",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config tools/webpack/production.js",
    "build:check": "run-s build server",
    "build:diff": "node tools/getBuildDiff",
    "analyze": "cross-env NODE_ENV=production webpack --progress --config tools/webpack/analyze.js",
    "clean": "run-p clean:*",
    "clean:dev": "rimraf .tmp",
    "clean:build": "rimraf build",
    "server": "http-server ./build -p 23000 -o --cors",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "safe-guard": "lint-staged",
    "lint": "run-p lint:*",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:script": "tslint -t stylish 'src/**/*.{ts,tsx}'",
    "fix": "run-p fix:*",
    "fix:style": "prettier --write 'src/**/*.css'",
    "fix:script": "tslint -t stylish --fix 'src/**/*.{ts,tsx}'",
    "fix:json": "prettier --write 'src/**/*.json'",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "precommit": "run-s typecheck safe-guard"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "libraries-frontend-framelunch": "framelunch/libraries-frontend-framelunch#0.4.0",
    "lodash-es": "^4.17.10",
    "rxjs": "^6.2.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "autoprefixer": "^9.1.3",
    "awesome-typescript-loader": "^5.2.0",
    "browser-sync": "^2.24.7",
    "cache-loader": "^1.1.0",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "dotenv": "^6.0.0",
    "fs-extra": "^7.0.0",
    "globby": "^8.0.1",
    "gulp": "^3.9.1",
    "gulp-ejs": "^3.1.3",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^6.0.0",
    "jest": "^23.5.0",
    "license-info-webpack-plugin": "^1.0.2",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.4.2",
    "npm-run-all": "^4.1.3",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.2",
    "raw-loader": "^0.5.1",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6"
  }
}
